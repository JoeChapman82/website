{% extends 'nunjucks/templates/base.njk' %}
{% import "nunjucks/macros/apis/navigation.njk" as navigation %}
{% import "nunjucks/macros/apis/typography.njk" as typography %}

{% block title %}API's - home{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="/css/apis.css">
{% endblock %}

{% block header %}
    {% include 'nunjucks/partials/topnav.njk' %}
{% endblock %}

{% block main %}
    <div class="apis-main-wrapper">
        {{ typography.mainIndexHeading('API\'s')}}
        <hr>
        {{ navigation.indexNavRow([
            {href: '/apis/uuid-generator', title: 'UUID Generator', img: '/images/apis/uuid-generator-3.png'}
        ]) }}
    </div>
{% endblock %}

{% block footer %}
    {% include "nunjucks/partials/footer.njk" %}
{% endblock %}

{% block scripts %}
{% endblock %}
